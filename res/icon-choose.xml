<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE p:layout>
<p:layout xmlns:p="https://github.com/DIG-/xml-to-iup/">
    <IupVbox>
        <IupFrame rastersize="300x400">
            <IupRadio p:name="icon_choose_radio">
                <IupScrollBox>
                    <IupGridBox p:name="icon_choose_grid" numdiv="3">
                        <IupToggle p:name="icon_choose_none" title="None" />
                        <IupToggle p:name="icon_choose_custom" title="Custom" />
                    </IupGridBox>
                </IupScrollBox>
            </IupRadio>
        </IupFrame>
        <IupHbox>
            <IupLabel expand="horizontal" />
            <IupButton p:name="icon_choose_select" title="&amp;Select" rastersize="60x0" />
            <IupLabel rastersize="10x0" />
            <IupButton p:name="icon_choose_cancel" title="&amp;Cancel" rastersize="60x0" />
        </IupHbox>
    </IupVbox>
</p:layout>